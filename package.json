{
  "name": "fast-file-explorer",
  "productName": "Fast File Explorer",
  "version": "25.10.04-dev.3",
  "description": "Fast File Explorer is a powerful tool for quickly navigating and managing your file system.",
  "main": ".vite/build/main.js",
  "config": {
    "forge": "forge.config.cjs"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run version:dev && electron-forge start",
    "package": "npm run version:beta && electron-forge package",
    "make": "npm run version:beta && electron-forge make",
    "publish": "npm run version:release && npm run package && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "update-deps": "ncu -u && npm install --force",
    "version:release": "node scripts/generate-version.js release",
    "version:beta": "node scripts/generate-version.js beta",
    "version:dev": "node scripts/generate-version.js dev",
    "version:show": "node -e \"console.log(require('./package.json').version)\""
  },
  "keywords": [],
  "author": {
    "name": "Erick Tran",
    "email": "erick.tran@etran.dev"
  },
  "license": "MIT",
  "dependencies": {
    "@electron-forge/plugin-vite": "^7.9.0",
    "@types/react": "^19.1.15",
    "@types/react-window": "^2.0.0",
    "drivelist": "^12.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "fast-transferlib": "^25.10.2-release.6",
    "node-disk-info": "^1.3.0",
    "npm-check-updates": "^19.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-window": "^2.1.2",
    "sass": "^1.93.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron/fuses": "^2.0.0",
    "@rollup/rollup-win32-x64-msvc": "^4.52.3",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react-dom": "^19.1.9",
    "electron": "38.2.0"
  }
}
