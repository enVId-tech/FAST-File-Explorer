{
  "name": "fast-file-explorer",
  "productName": "Fast File Explorer",
  "version": "26.01.06-dev.3",
  "description": "Fast File Explorer is a powerful tool for quickly navigating and managing your file system.",
  "main": ".vite/build/main.js",
  "config": {
    "forge": "forge.config.cjs"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run version:dev && electron-forge start",
    "package": "npm run version:beta && electron-forge package",
    "make": "npm run version:beta && electron-forge make",
    "publish": "npm run version:release && npm run package && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "update-deps": "npx npm-check-updates -u && npm install --force",
    "version:release": "npm-v release",
    "version:beta": "npm-v beta",
    "version:dev": "npm-v dev",
    "version:show": "node -e \"console.log(require('./package.json').version)\"",
    "ncu": "npx npm-check-updates -u && npm install --force"
  },
  "keywords": [],
  "author": {
    "name": "Erick Tran",
    "email": "erick.tran@etran.dev"
  },
  "license": "MIT",
  "dependencies": {
    "@electron-forge/plugin-vite": "^7.11.1",
    "@types/react": "^19.2.10",
    "@types/react-window": "^2.0.0",
    "dotenv": "^17.2.3",
    "drivelist": "^12.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "fast-transferlib": "^25.10.2-release.6",
    "node-disk-info": "^1.3.0",
    "npm-check-updates": "^19.3.2",
    "npm-version-lib": "^25.12.26-release.17",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-window": "^2.2.6",
    "sass": "^1.97.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron/fuses": "^2.0.0",
    "@rollup/rollup-win32-x64-msvc": "^4.57.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react-dom": "^19.2.3",
    "electron": "40.1.0"
  }
}