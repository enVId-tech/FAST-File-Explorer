{
  "name": "fast-file-explorer",
  "productName": "Fast File Explorer",
  "version": "25.08.12.2-release",
  "description": "Fast File Explorer is a powerful tool for quickly navigating and managing your file system.",
  "main": ".vite/build/main.js",
  "config": {
    "forge": "forge.config.cjs"
  },
  "scripts": {
    "prebuild": "node scripts/generate-version.js",
    "prestart": "node scripts/generate-version.js",
    "prepackage": "node scripts/generate-version.js",
    "premake": "node scripts/generate-version.js",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "update-deps": "ncu -u && npm install",
    "version:release": "node scripts/generate-version.js release",
    "version:beta": "node scripts/generate-version.js beta",
    "version:alpha": "node scripts/generate-version.js alpha",
    "version:show": "node -e \"console.log(require('./package.json').version)\""
  },
  "keywords": [],
  "author": {
    "name": "Erick Tran",
    "email": "thelittlebotengineer@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@electron-forge/plugin-vite": "^7.8.3",
    "@types/react": "^19.1.10",
    "drivelist": "^12.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "node-disk-info": "^1.3.0",
    "npm-check-updates": "^18.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "sass": "^1.90.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron/fuses": "^2.0.0",
    "@rollup/rollup-win32-x64-msvc": "^4.46.2",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react-dom": "^19.1.7",
    "electron": "37.2.6"
  }
}
